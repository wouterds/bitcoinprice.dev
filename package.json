{
  "name": "bitcoinprice-dev",
  "version": "1.2.0",
  "private": true,
  "repository": "github:wouterds/bitcoinprice.dev",
  "scripts": {
    "start": "cd ./dist && node -r ./bootstrap.js ./main.js",
    "dev:docker:start": "docker-compose -f $(npm prefix)/.docker/docker-compose.dev.yml up -d --remove-orphans",
    "dev:node": "nodemon",
    "dev": "yarn dev:docker:start && sleep 3 && yarn dev:node",
    "build": "tsc",
    "lint": "eslint --ext '.ts, js' . && tsc",
    "lint:fix": "eslint --ext '.ts, js' --fix ."
  },
  "dependencies": {
    "chalk": "^4.1.0",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "node-fetch": "^2.6.0",
    "redis": "^3.0.2",
    "tsconfig-paths": "^3.9.0",
    "ws": "^7.3.1"
  },
  "devDependencies": {
    "@types/express": "^4.17.7",
    "@types/node": "^14.6.2",
    "@types/node-fetch": "^2.5.7",
    "@types/redis": "^2.8.26",
    "@types/ws": "^7.2.6",
    "@typescript-eslint/eslint-plugin": "^3.10.1",
    "@typescript-eslint/parser": "^3.10.1",
    "eslint": "^7.7.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-prettier": "^3.1.4",
    "eslint-plugin-simple-import-sort": "^5.0.3",
    "husky": "^4.2.5",
    "lint-staged": "^10.2.13",
    "nodemon": "^2.0.4",
    "prettier": "^2.1.1",
    "ts-node": "^9.0.0",
    "typescript": "^4.0.2"
  }
}
