{
  "name": "bitcoinprice-dev",
  "version": "2.0.0",
  "license": "MIT",
  "repository": "github:wouterds/bitcoinprice.dev",
  "scripts": {
    "start": "cd ./dist && node -r ./bootstrap.js ./main.js",
    "ticker:binance": "cd ./dist && node -r ./bootstrap.js ./scripts/binance-ticker.js",
    "ticker:bitfinex": "cd ./dist && node -r ./bootstrap.js ./scripts/bitfinex-ticker.js",
    "ticker:bitstamp": "cd ./dist && node -r ./bootstrap.js ./scripts/bitstamp-ticker.js",
    "ticker:coinbase": "cd ./dist && node -r ./bootstrap.js ./scripts/coinbase-ticker.js",
    "ticker:kraken": "cd ./dist && node -r ./bootstrap.js ./scripts/kraken-ticker.js",
    "minutely-average:sample": "cd ./dist && node -r ./bootstrap.js ./scripts/minutely-average-sample.js",
    "minutely-average:clean": "cd ./dist && node -r ./bootstrap.js ./scripts/minutely-average-clean.js",
    "dev:docker:start": "docker-compose -f \"$(npm prefix)/.docker/docker-compose.dev.yml\" up -d --remove-orphans",
    "dev:node": "nodemon",
    "dev": "yarn dev:docker:start && sleep 3 && yarn dev:node",
    "build": "tsc",
    "lint": "eslint --ext '.ts, js' . && tsc",
    "lint:fix": "eslint --ext '.ts, js' --fix ."
  },
  "dependencies": {
    "chalk": "^4.1.0",
    "cors": "^2.8.5",
    "date-fns": "^2.16.1",
    "dd-trace": "^0.26.0",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "node-fetch": "^2.6.1",
    "redis": "^3.0.2",
    "tsconfig-paths": "^3.9.0",
    "ws": "^7.3.1"
  },
  "devDependencies": {
    "@types/cors": "^2.8.7",
    "@types/express": "^4.17.8",
    "@types/node": "^14.10.1",
    "@types/node-fetch": "^2.5.7",
    "@types/redis": "^2.8.27",
    "@types/ws": "^7.2.6",
    "@typescript-eslint/eslint-plugin": "^3.10.1",
    "@typescript-eslint/parser": "^3.10.1",
    "eslint": "^7.9.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-prettier": "^3.1.4",
    "eslint-plugin-simple-import-sort": "^5.0.3",
    "husky": "^4.3.0",
    "lint-staged": "^10.3.0",
    "nodemon": "^2.0.4",
    "prettier": "^2.2.1",
    "ts-node": "^9.0.0",
    "typescript": "^4.0.2"
  }
}
